    var systemData = new SystemLimpet("rocketcontentapi");
    var appThemeSystem = new AppThemeSystemLimpet(systemData.SystemKey);
    var info = articleData.Info;
    var appTheme = (AppThemeLimpet)Model.GetDataObject("apptheme");
    var remoteModule = (RemoteModule)Model.GetDataObject("remotemodule");
    var sessionParams = Model.SessionParamsData;
}

<link rel="stylesheet" href="/DesktopModules/DNNrocket/API/Themes/config-w3/1.0/css/dnnrocket-theme.css">

